
    <br>
    <br>
    <div class="container">
        <div id="contenido" class="">
            <table class="mdl-data-table mdl-js-data-table mdl-shadow--2dp full-width table-responsive" id="tablaProduct">
						<thead>
							<tr>
								<th>Código del Producto</th>
								<th>Descripción</th>
								<th>Stock</th>
								<th>Precio de Compra</th>
								<th>Precio de Venta</th>
								<th><center>Opción</center></th>
							</tr>
						</thead>
						<tbody id="contenidoTabla">
        <div>{{#each data}}

            
							<tr>
							<td id="{{this.PRDIDENTI}}" >{{this.PRDIDENTI}}</td>
							<td>{{this.PRDNOMBRE}}</td>
							<td>{{this.PUBSTOCK}}</td>
							<td>{{this.PrecComp_Pro}}</td>
							<td>{{this.PrecVen_Pro}}</td>
							<td>
								<div>
									<center>
									<button title="Más Información" class="mdl-button mdl-button--icon mdl-js-button mdl-js-ripple-effect" onclick="infoProductos(event,this)"><i style="color:#FF9800" class="zmdi zmdi-info"></i></button>&nbsp;&nbsp;&nbsp;
									<button title="modificar" class="mdl-button mdl-button--icon mdl-js-button mdl-js-ripple-effect" onclick="editProducto(event,this)"><i style="color:#3f51b5" class="zmdi zmdi-edit"></i></button>&nbsp;&nbsp;&nbsp;
									<button onclick="deleteProduct(event,this)" id="borrarClient" title="borrar" class="mdl-button mdl-button--icon mdl-js-button mdl-js-ripple-effect"><i style="color:#B71C1C" class="zmdi zmdi-delete"></i></button>
									</center>
								</div>
							</td>
							</tr>
            {{/each}}
            </tbody>
					</table>
        </div>
        </div>


        
        
